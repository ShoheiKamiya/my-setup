[alias]
  cm = commit
  br = branch
  co = checkout
  df = diff --name-only
  dfs = diff --staged
  st = status
  logs = log --oneline -n 5
[core]
	editor = vim
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
